{
  "name": "concretecms/nightcap",
  "description": "Easily build PHP client libraries for your REST APIs.",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andrew Embler",
      "email": "andrew@concrete5.org",
      "homepage": "http://andrewembler.com"
    }
  ],
  "require": {
    "concrete5/oauth2-concrete5": "^1.0",
    "kamermans/guzzle-oauth2-subscriber": "^1.0",
    "guzzlehttp/guzzle-services": "^1.1",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "rtheunissen/guzzle-log-middleware": "dev-master",
    "psr/log": "^1.1@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Concrete\\Nightcap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Concrete\\Nightcap\\Test\\": "tests/src/"
    }
  }
}
